<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--#region Window Backgrounds-->
    <SolidColorBrush x:Key="scb_Primary" Color="#138198"/>
    <SolidColorBrush x:Key="scb_Secondary" Color="#275D75"/>
    <SolidColorBrush x:Key="scb_Tertiary" Color="#153340"/>
    <!--#endregion-->
    <!--#region Elements-->
    <SolidColorBrush x:Key="scb_White" Color="White"/>
    <SolidColorBrush x:Key="scb_Black" Color="Black"/>
    <SolidColorBrush x:Key="scb_Red" Color="Red"/>
   
    <SolidColorBrush x:Key="scb_Element" Color="#ffab5e"/>
    <SolidColorBrush x:Key="scb_ElementMouseOver" Color="#FF9635"/>
    <SolidColorBrush x:Key="scb_ElementPressed" Color="#FFC56C19"/>

    <SolidColorBrush x:Key="scb_Watermark" Color="#FF969696"/>
    <!--#endregion-->
</ResourceDictionary>