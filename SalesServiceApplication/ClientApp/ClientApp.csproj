<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Icons\Icon.ico" />
    <None Remove="Resources\Logos\Logo.png" />
    <None Remove="Resources\Styles\Pictures\Window\Collapse.png" />
    <None Remove="Resources\Styles\Pictures\Window\Collapse_Active.png" />
    <None Remove="Resources\Styles\Pictures\Window\Collapse_Pressed.png" />
    <None Remove="Resources\Styles\Pictures\Window\Exit.png" />
